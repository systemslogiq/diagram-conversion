# Input directory containing source images
INPUT_PATH=./input

# Output directory for converted images
OUTPUT_PATH=./output

# Optional: File prefix filter (leave empty to process all files)
PREFIX=

# Conversion settings
QUALITY=80
EFFORT=6
FORMAT=webp

# Processing options
CONCURRENCY=4
RECURSIVE=false
DRY_RUN=false

# Transparency settings
REMOVE_WHITE_BG=true
TRANSPARENCY_FUZZ=20

# Logging
LOG_LEVEL=info